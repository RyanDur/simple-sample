{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "google"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
  },
  "overrides": [
    {
      "files": ["*.json"],
      "rules": {
        "quotes": ["error", "double"],
	"semi": ["error", "never"],
	"comma-dangle": ["error", "never"]
      }
    }
  ]
}
